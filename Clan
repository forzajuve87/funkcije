package funkcije_vezba;

public class Clan {
	
	private int a, b;
	
	// kostruktori
	public Clan(int a, int b) {
		this.a = a;
		this.b = b;
	}
	public Clan(final Clan kopija) {
		this(kopija.a, kopija.b);
	}
	
	// set i get metode
	public void setA(int a) {
		this.a = a;
	}
	public void setB(int b) {
		this.b = b;
	}
	public int getA() {
		return this.a;
	}
	public int getB() {
		return this.b;
	}
	
	public String toString() {
		return "Clan:a=" + this.a + ", b=" + this.b + ".";
	}
}
